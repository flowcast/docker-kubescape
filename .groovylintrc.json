{
    "extends": "recommended",
    "rules": {
        "BlockEndsWithBlankLine": {
            "enabled": false
        },
        "BlockStartsWithBlankLine": {
            "enabled": false
        },
        "ClassEndsWithBlankLine": {
            "enabled": false
        },
        "ClassJavadoc": {
            "enabled": false
        },
        "CompileStatic": {
            "enabled": false
